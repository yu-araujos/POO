package br.fiap.funcionario;

import br.fiap.cargo.Cargo;

public class Funcionario {
	private String nome;
	private double salario;
	private Cargo cargo;
	
	public Funcionario(String nome, double salario, Cargo cargo) {
		super();
		this.nome = nome;
		this.salario = salario;
		this.cargo = cargo;
	}
}
